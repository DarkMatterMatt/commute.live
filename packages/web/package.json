{
    "name": "@commutelive/web",
    "version": "2.5.4",
    "description": "The main frontend of [Commute.live](https://commute.live), which provides a user-friendly interface to view vehicles on one or more public transport routes.",
    "main": "src/ts/index.ts",
    "author": "Matt Moran",
    "license": "GPL-3.0",
    "homepage": "https://github.com/DarkMatterMatt/commute.live/packages/web#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DarkMatterMatt/commute.live.git"
    },
    "bugs": {
        "url": "https://github.com/DarkMatterMatt/commute.live/issues"
    },
    "scripts": {
        "build": "webpack --env prod --mode production",
        "build:docker": "docker build -f Dockerfile -t commute.live-web ../../",
        "clean": "rimraf dist",
        "clean:docker": "docker rm commute.live-web || echo\\",
        "lint": "eslint src/ts/** --fix",
        "percy": "node test/snapshot.mjs dist",
        "start": "webpack serve --env dev --mode development",
        "start:docker": "docker run --name commute.live-web -dp 8080:80 commute.live-web",
        "stop:docker": "docker kill --signal=SIGINT commute.live-web || echo\\",
        "typecheck": "tsc --noEmit"
    },
    "nx": {
        "targets": {
            "build": {
                "dependsOn": [
                    "^build",
                    "typecheck"
                ],
                "outputs": [
                    "{projectRoot}/dist"
                ]
            },
            "build:docker": {
                "dependsOn": [
                    "^build"
                ],
                "outputs": []
            },
            "clean:docker": {
                "dependsOn": [
                    "stop:docker"
                ],
                "outputs": []
            },
            "lint": {
                "dependsOn": [
                    "^build"
                ],
                "outputs": []
            },
            "percy": {
                "dependsOn": [
                    "build"
                ],
                "outputs": []
            },
            "start": {
                "dependsOn": [
                    "typecheck"
                ],
                "outputs": []
            },
            "start:docker": {
                "dependsOn": [
                    "clean:docker",
                    "build:docker"
                ],
                "outputs": []
            },
            "typecheck": {
                "dependsOn": [
                    "^build"
                ],
                "outputs": []
            }
        }
    },
    "engines": {
        "node": "^20"
    },
    "dependencies": {
        "@babel/runtime": "^7.24.4",
        "@commutelive/common": "^2.5.3",
        "@simonwep/pickr": "^1.9.0",
        "@types/color-convert": "^2.0.3",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "bezier-easing": "^2.1.0",
        "color-convert": "^2.0.1",
        "core-js": "^3.37.0",
        "lz-string": "^1.5.0",
        "regenerator-runtime": "^0.14.1"
    },
    "devDependencies": {
        "@babel/core": "^7.24.4",
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/preset-env": "^7.24.4",
        "@babel/preset-typescript": "^7.24.1",
        "@percy/cli": "^1.28.3",
        "@percy/puppeteer": "^2.0.2",
        "@types/google.maps": "^3.55.7",
        "@types/lz-string": "~1.3.34",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "autoprefixer": "^10.4.19",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "caniuse-lite": "^1.0.30001612",
        "clean-webpack-plugin": "^4.0.0",
        "cname-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.1",
        "dotenv": "^16.4.5",
        "dotenv-webpack": "^8.1.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "express": "^4.19.2",
        "file-loader": "^6.2.0",
        "get-port": "^7.1.0",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.9.0",
        "normalize.css": "^8.0.1",
        "postcss-loader": "^8.1.1",
        "puppeteer": "^22.7.0",
        "rimraf": "^5.0.5",
        "sass": "^1.75.0",
        "sass-loader": "^14.2.1",
        "sharp": "^0.33.3",
        "string-replace-loader": "^3.1.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-md5-hash": "0.0.6",
        "webpack-pwa-manifest": "^4.3.0",
        "webpack-stream": "^7.0.0",
        "workbox-webpack-plugin": "^7.0.0"
    }
}
