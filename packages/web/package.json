{
    "name": "@commutelive/web",
    "version": "2.2.1",
    "description": "The main frontend of [Commute.live](https://commute.live), which provides a user-friendly interface to view vehicles on one or more public transport routes.",
    "main": "src/ts/index.ts",
    "author": "Matt Moran",
    "license": "GPL-3.0",
    "homepage": "https://github.com/DarkMatterMatt/commute.live/packages/web#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DarkMatterMatt/commute.live.git"
    },
    "bugs": {
        "url": "https://github.com/DarkMatterMatt/commute.live/issues"
    },
    "scripts": {
        "build": "webpack --env prod --mode production",
        "clean": "rimraf dist",
        "lint": "eslint src/ts/** --fix",
        "start": "webpack serve --env dev --mode development",
        "typecheck": "tsc --noEmit"
    },
    "nx": {
        "targets": {
            "build": {
                "dependsOn": [
                    "^build",
                    "typecheck"
                ],
                "outputs": [
                    "{projectRoot}/dist"
                ]
            },
            "lint": {
                "dependsOn": [
                    "^build"
                ],
                "outputs": []
            },
            "start": {
                "dependsOn": [
                    "typecheck"
                ],
                "outputs": []
            },
            "typecheck": {
                "dependsOn": [
                    "^build"
                ],
                "outputs": []
            }
        }
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@babel/runtime": "^7.20.7",
        "@commutelive/common": "^2.4.1",
        "@simonwep/pickr": "^1.8.2",
        "@types/color-convert": "^2.0.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "bezier-easing": "^2.1.0",
        "color-convert": "^2.0.1",
        "core-js": "^3.27.1",
        "lz-string": "^1.4.4",
        "regenerator-runtime": "^0.13.11"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/googlemaps": "^3.43.3",
        "@types/lz-string": "^1.3.34",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "autoprefixer": "^10.4.13",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^8.3.0",
        "browser-sync": "^2.27.11",
        "clean-webpack-plugin": "^3.0.0",
        "cname-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.2.7",
        "dotenv-webpack": "^6.0.4",
        "eslint": "^8.31.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^6.2.0",
        "get-port": "^5.1.1",
        "html-webpack-plugin": "^4.5.2",
        "mini-css-extract-plugin": "^1.6.2",
        "normalize.css": "^8.0.1",
        "postcss-loader": "^4.3.0",
        "rimraf": "^3.0.2",
        "sass": "^1.57.1",
        "sass-loader": "^10.4.1",
        "string-replace-loader": "^3.1.0",
        "ts-loader": "^8.4.0",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^3.11.3",
        "webpack-md5-hash": "0.0.6",
        "webpack-pwa-manifest": "^4.3.0",
        "webpack-stream": "^6.1.2",
        "workbox-webpack-plugin": "^6.5.4"
    }
}
